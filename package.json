{
  "name": "quizknow-app4",
  "version": "1.0.0",
  "description": "A comprehensive quiz management system with Student, Instructor, and Admin roles",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "server-test": "nodemon server-test.js",
    "client": "cd client && npm start",
    "build": "cd client && npm install && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "@vercel/blob": "^2.0.0",
    "axios": "^1.13.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "mongodb-memory-server": "^10.2.0",
    "nodemon": "^3.0.1"
  }
}
